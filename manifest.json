{
    "name": "myNewTab",
    "version": "1.1.2",
    "description": "自定义chrome标签页",
    "icons": {
        "128": "img/zwei_128.png"
    },
    "chrome_url_overrides": {
      "newtab": "newtab.html"
    },
    "homepage_url": "https://github.com/Stareven233/myNewTab",
    "browser_action": {
        "default_icon": {        
            "128": "img/zwei_128.png"
        },
        "default_title": "myNewTab"
    },
    "background": {
        "scripts": [
            "/js/jquery.min.js"
        ]
    },
    "options_page": "",
    "permissions": [
        "tabs"
    ],
    "content_security_policy": "script-src 'self'; style-src 'self' 'unsafe-inline'; img-src 'self' https://raw.githubusercontent.com; object-src 'self'",
    "minimum_chrome_version": "86",
    "manifest_version": 2,
    "update_url": "https://github.com/Stareven233/myNewTab"
}
